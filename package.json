{
  "name": "@kakuilan/video-react",
  "type": "module",
  "version": "1.0.5",
  "author": "kakuilan",
  "license": "Apache-2.0",
  "description": "Video.js component for React",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kakuilan/video-react.git"
  },
  "bugs": {
    "url": "https://github.com/kakuilan/video-react/issues"
  },
  "homepage": "https://github.com/kakuilan/video-react#readme",
  "files": [
    "dist"
  ],
  "keywords": [
    "video.js",
    "typescript",
    "react",
    "hook"
  ],
  "scripts": {
    "prepare": "husky install",
    "start:cjs": "node ./dist/index.cjs",
    "start:esm": "node ./dist/index.mjs",
    "dev": "nodemon",
    "develop": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "clean": "rimraf build && rimraf dist",
    "build": "swc ./src -d build",
    "build:watch": "swc ./src -d build -w",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --ext .tsx --fix",
    "prettier": "prettier '**/*.{ts,tsx,json,md}'",
    "prettier:write": "prettier --write '**/*.{ts,tsx,json,md}'",
    "type-check": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "release": "semantic-release",
    "bundle": "unbuild"
  },
  "dependencies": {
    "dequal": "^2.0.3",
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.0",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "@swc/cli": "0.3.10",
    "@swc/core": "1.4.6",
    "@swc/jest": "0.2.36",
    "@testing-library/react": "^14.2.2",
    "@types/jest": "29.5.12",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/node": "20.11.26",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@types/video.js": "^7.3.57",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.0",
    "jest": "29.7.0",
    "lint-staged": "^15.0.0",
    "nodemon": "3.1.0",
    "prettier": "3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.14.0",
    "rimraf": "5.0.5",
    "semantic-release": "^23.0.0",
    "ts-node": "^10.9.1",
    "typescript": "5.4.2",
    "unbuild": "^2.0.0"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0"
  },
  "peerDependencies": {
    "@videojs/http-streaming": "^3.10.0",
    "video.js": "^8.10.0",
    "videojs-contrib-ads": "^7.5.0"
  }
}
